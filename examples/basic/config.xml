<?xml version='1.0' encoding='utf-8'?>
<widget id="hello.admob" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello AdMob</name>
    <description>
        A sample cordova-plugin-admob-tomitank application.
    </description>
    <author email="tomitank@users.noreply.github.com" href="https://github.com/tomitank/cordova-plugin-admob-tomitank">
        tomitank
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-admob-tomitank" spec="~2.3.0" />
    <engine name="android" spec="8.0.0" />
    <engine name="ios" spec="~5.0.0" />
</widget>
